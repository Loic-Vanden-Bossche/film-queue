# Redis
REDIS_URL=redis://localhost:6379
REDIS_EVENTS_CHANNEL=download-events

# Worker settings
MAX_CONCURRENT=1
PUPPETEER_TIMEOUT=45000
PUPPETEER_HEADLESS=true
# PUPPETEER_EXECUTABLE_PATH=/path/to/chrome

# dailyuploads.net auth
DAILYUPLOADS_USER=
DAILYUPLOADS_PASS=
DAILYUPLOADS_LOGIN_URL=https://dailyuploads.net/login
DAILYUPLOADS_COOKIE_PATH=apps/worker/.session/dailyuploads.json

# Worker heartbeat
WORKER_HEARTBEAT_KEY=download-worker:heartbeat
WORKER_HEARTBEAT_TTL=15
WORKER_HEARTBEAT_INTERVAL=5
DOWNLOAD_REQUEST_TIMEOUT_MS=60000
FOLDER_STATS_INTERVAL_MS=30000

# Jellyfin
JELLYFIN_URL=http://localhost:8096
JELLYFIN_API_KEY=
